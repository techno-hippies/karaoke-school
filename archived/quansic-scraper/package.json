{
  "name": "quansic-scraper",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run src/index.ts",
    "scrape": "bun run src/scraper.ts",
    "test": "bun test",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:ingest:grimes": "bun run src/db/ingest.ts --grimes",
    "db:ingest:all": "bun run src/db/ingest.ts --all",
    "check:artists": "bun run scripts/check/check-artists.ts",
    "check:data": "bun run scripts/check/check-data.ts",
    "check:complete": "bun run scripts/check/check-complete-data.ts",
    "check:visual": "bun run scripts/check/check-visual.ts",
    "check:wikidata": "bun run scripts/check/check-wikidata.ts",
    "verify:artists": "bun run scripts/verify/verify-artists.ts",
    "verify:complete": "bun run scripts/verify/verify-complete.ts",
    "analyze": "bun run scripts/analyze/analyze-data.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@graphprotocol/grc-20": "^0.24.1",
    "@playwright/test": "^1.48.0",
    "@types/pg": "^8.15.5",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.5",
    "kuzu": "^0.11.2",
    "pg": "^8.16.3",
    "playwright": "^1.48.0",
    "viem": "^2.37.6",
    "zod": "^4.1.9"
  }
}