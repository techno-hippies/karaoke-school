{
  "name": "@karaoke-school/grc20-integration",
  "version": "1.0.0",
  "description": "GRC-20 knowledge graph integration for karaoke learning platform",
  "main": "index.ts",
  "scripts": {
    "setup": "dotenvx run -f .env -- bun run scripts/01-setup-space.ts",
    "define-types": "dotenvx run -f .env -- bun run scripts/02-define-types.ts",
    "validate": "dotenvx run -f .env -- bun run scripts/validate-catalog.ts",
    "enrich-mb": "dotenvx run -f .env -- bun run scripts/enrich-musicbrainz.ts",
    "corroborate": "dotenvx run -f .env -- bun run scripts/run-corroboration.ts",
    "import-artists": "dotenvx run -f .env -- bun run scripts/03-import-artists.ts",
    "import-works": "dotenvx run -f .env -- bun run scripts/03-import-works.ts",
    "import-segments": "dotenvx run -f .env -- bun run scripts/04-import-segments.ts",
    "link-musicbrainz": "dotenvx run -f .env -- bun run scripts/05-link-musicbrainz.ts",
    "query-test": "dotenvx run -f .env -- bun run scripts/test-queries.ts"
  },
  "dependencies": {
    "@graphprotocol/grc-20": "^0.25.5",
    "viem": "^2.0.0",
    "postgres": "^3.4.3",
    "@dotenvx/dotenvx": "^1.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}
